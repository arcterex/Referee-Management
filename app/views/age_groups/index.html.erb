<h1>Listing age_groups</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
  </tr>

<% @age_groups.each do |age_group| %>
  <tr>
    <td><%=h age_group.name %></td>
    <td><%=h age_group.description %></td>
    <td><%= link_to 'Show', age_group %></td>
    <td><%= link_to 'Edit', edit_age_group_path(age_group) %></td>
    <td><%= link_to 'Destroy', age_group, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New age_group', new_age_group_path %>